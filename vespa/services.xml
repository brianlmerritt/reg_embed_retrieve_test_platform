<?xml version="1.0" encoding="utf-8" ?>
<services version="1.0">
  <container id="vetsearch_container" version="1.0">
    <search />
    <document-api />
    <nodes>
      <node hostalias="node1" />
    </nodes>
  </container>

  <content id="vetsearch" version="1.0">
    <redundancy>1</redundancy>
    <documents>
      <document type="vetsearch" mode="index" />
    </documents>
    <nodes>
      <node hostalias="node1" distribution-key="0" />
    </nodes>
  </content>

  <hosts>
    <host name="localhost" alias="node1" />
  </hosts>
</services>